{
  "name": "react_redux_starter",
  "version": "1.0.0",
  "description": "A Simple react redux starter app. Showing how to set React with Redux, modularized SASS and ESLint.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev-server lint:watch",
    "dev-server": "babel-node tools/devServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "remove-gzip": "node_modules/.bin/rimraf dist_gzip",
    "precompress": "npm run remove-gzip && mkdir dist_gzip",
    "compress": "babel-node tools/compress.js"
  },
  "keywords": [
    "react",
    "redux",
    "sass",
    "eslint",
    "webpack"
  ],
  "author": "Henrique Coura",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "tether": "^1.4.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {}
}
